<h1>Humain</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Prenom</th>
            <td>{{ entity.prenom }}</td>
        </tr>
        <tr>
            <th>Nom</th>
            <td>{{ entity.nom }}</td>
        </tr>
        <tr>
            <th>Datenaissance</th>
            <td>{{ entity.dateNaissance|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Sexe</th>
            <td>{{ entity.sexe }}</td>
        </tr>
        <tr>
            <th>Orientationsexuelle</th>
            <td>{{ entity.orientationSexuelle }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('admin_humain') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('admin_humain_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('admin_humain_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
